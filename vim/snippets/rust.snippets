snippet /       单行注释
	// ${0}
snippet //      多行注释
	/* ${0}*/
snippet -       分隔注释
	//----------------------------------------------------------------------${0}
# 函数、复合结构
snippet main    主函数
	fn main () {
	    ${0}
	}
# 单个关键字
snippet l       变量赋值
	let ${1} = ${2};${0}
snippet m       使变量可变
	mut ${0}
snippet lm      声明可变变量
	let mut ${1} = ${2};${0}
# 输入输出
snippet p       输出（不换行）
	print!("${1}"${0});
snippet pl      输出（换行）
	println!("${1}"${0});
